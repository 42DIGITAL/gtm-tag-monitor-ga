___SANDBOXED_JS_FOR_WEB_TEMPLATE___

/*
  source: 
    https://developers.google.com/tag-manager/templates/monitoring
*/

/*
  loading required APIs
*/
const addEventCallback = require('addEventCallback');
const getUrl = require('getUrl');
const getCookieValues = require('getCookieValues');
const makeString = require('makeString');
const encodeUri = require('encodeUri');
const sendPixel = require('sendPixel');

addEventCallback(function(ctid, eventData) {
    /*
    event data object contains arbitrary key value pairs in a tag definition,
    which will be included in this object when an event callback occurs.
  */
  const tags = eventData.tags;
  const nameORid = data.nameORid;
  const tagNameKey = data.tagNameKey;
  for (let i in tags) {
    if(nameORid == 'tagName' && tags[i][tagNameKey] === undefined){
      /*
        filtering out tags that their meta data name is not set.
      */
      continue;
    }else{
      let requestURL = 'https://www.google-analytics.com/collect?';
      let payload = 'v=1&t=event' +
          /*
            making this a non-interactive event.
          */
          '&ni=1' +
          /*
            if using GA API is intended, it's better to use a consistent category name.
          */
          '&ec=' + data.eventCategory +
          '&tid=' + data.trackingID +
          /*
            getting client id from _ga cookie.
          */
          '&cid=' + makeString(getCookieValues('_ga')).split('.').slice(-2).join('.') + 
          '&ctid=' + ctid + //container_id
          /*
            page path as event label
          */
          '&el=' + getUrl('path');
      /*
          tag name or id as event action.
      */
      if(nameORid == 'tagName'){
      payload +=
        '&ea=' + makeString(tags[i][tagNameKey]);
      }else{
        payload +=
        	'&ea=' + makeString(tags[i].id);
      }
      requestURL += payload;
      /*
        making a GET request to the specified URL endpoint (GA).
      */
      sendPixel(encodeUri(requestURL));
    }
  }
});

data.gtmOnSuccess();


